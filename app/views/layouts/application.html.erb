<!DOCTYPE html>
<html>
<head>
  <title>Todolists</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'shared/navigation' %>

<div class="container">



<% flash.each do |key, value| %>
  <p id='<%= key %>'><%= value %></p>
<% end %>

<%= yield %>

</div>

</body>
</html>
